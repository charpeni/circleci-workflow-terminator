{
  "name": "Auto-cancel CircleCI redundant builds",
  "description":
    "Used to auto-cancel redundant builds based on GitHub push event",
  "repository":
    "https://github.com/charpeni/circleci-workflows-cancel-redundant-builds",
  "keywords": ["node", "github", "circleci"],
  "image": "heroku/nodejs",
  "env": {
    "CIRCLE_TOKEN": {
      "description": "CircleCI Token",
      "value": ""
    },
    "MAX_RETRIES": {
      "description": "Max retries to verify if the commit is on CircleCI",
      "value": "",
      "required": false
    },
    "WAIT_TIME": {
      "description": "Wait time between each retry",
      "value": "",
      "required": false
    }
  }
}
